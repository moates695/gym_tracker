{
  "cli": {
    "version": ">= 2.0.0"
  },
  "build": {
    "development": {
      "developmentClient": false, 
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest",
        "gradleCommand": ":app:assembleRelease -x lint"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://subtly-ample-bluebird.ngrok-free.app",
        "EXPO_API_TIMEOUT_MS": "5000"
      }
    },
    "preview": {
      "developmentClient": false, 
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest",
        "gradleCommand": ":app:assembleRelease -x lint"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://gymjunkie.moates.com.au",
        "EXPO_API_TIMEOUT_MS": "5000"
      }
    },
    "internal_bundle": {
      "channel": "internal",
      "distribution": "store",
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://gymjunkie.moates.com.au",
        "EXPO_API_TIMEOUT_MS": "5000"
      }
    }
  }
}