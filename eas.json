{
  "cli": {
    "version": ">= 2.0.0"
  },
  "build": {
    "development": {
      "developmentClient": false, 
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest",
        "gradleCommand": ":app:assembleRelease -x lint"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://subtly-ample-bluebird.ngrok-free.app",
        "USE_REAL": "false"
      }
    },
    "testing": {
      "developmentClient": false, 
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest",
        "gradleCommand": ":app:assembleRelease -x lint"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://subtly-ample-bluebird.ngrok-free.app",
        "USE_REAL": "true"
      }
    }
  }
}